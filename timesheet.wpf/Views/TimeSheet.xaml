<Window x:Class="timesheet.wpf.Views.TimeSheet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:timesheet.wpf.Views"
        xmlns:local1="clr-namespace:timesheet.wpf.ViewModel"
        mc:Ignorable="d"
        Title="TimeSheet" Height="450" Width="800">
    <Window.DataContext>
        <local1:TimeSheetViewModel>
            
        </local1:TimeSheetViewModel>
    </Window.DataContext>
    <Grid Grid.Row="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <DataGrid Grid.Row="1" ItemsSource="{Binding employees,UpdateSourceTrigger=PropertyChanged}" AutoGenerateColumns="True">
            
        </DataGrid>
    </Grid>
</Window>
